        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/js-yaml": "^4.0.9",
        "@types/unist": "^3.0.2",
        "arktype": "1.0.29-alpha",
        "js-yaml": "^4.1.0",
        "remark": "^15.0.1",
        "remark-frontmatter": "^5.0.0",
        "remark-parse": "^11.0.0",
